{
  "name": "lejing-dashboard-pro",
  "description": "Lejing Dashboard Pro",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "local": "vite --mode developer_local --port 8080 --host",
    "dev": "vite --mode development --port 8080 --host",
    "build:local": "tsc && vite build --mode developer_local && rm -fv tsconfig*.tsbuildinfo",
    "build:dev": "tsc && vite build --mode development && rm -fv tsconfig*.tsbuildinfo",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "ahooks": "^3.8.1",
    "antd": "^5.21.5",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "echarts": "^5.5.1",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-router-dom": "^6.27.0",
    "resso": "^0.17.0",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@eslint/js": "9.13.0",
    "@types/lodash": "4.17.12",
    "@types/node": "22.8.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.3",
    "eslint": "9.13.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}

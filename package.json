{
  "name": "lejing-dashboard-pro",
  "description": "Lejing Dashboard Pro",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "local": "vite --mode developer_local --port 8080 --host",
    "dev": "vite --mode development --port 8080 --host",
    "build:local": "tsc && vite build --mode developer_local && rm -fv tsconfig*.tsbuildinfo",
    "build:dev": "tsc && vite build --mode development && rm -fv tsconfig*.tsbuildinfo",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "ahooks": "^3.8.4",
    "antd": "^5.24.6",
    "axios": "1.8.2",
    "date-fns": "^4.1.0",
    "echarts": "^5.6.0",
    "less": "^4.3.0",
    "less-loader": "^12.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.5.0",
    "react-draggable": "^4.4.6",
    "resso": "^0.17.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@types/lodash": "^4.17.16",
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "9.14.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.5"
  }
}

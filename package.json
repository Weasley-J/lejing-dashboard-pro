{
  "name": "lejing-dashboard-pro",
  "description": "Lejing Dashboard Pro",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "local": "vite --mode developer_local --port 8080 --host",
    "dev": "vite --mode development --port 8080 --host",
    "build:local": "tsc && vite build --mode developer_local && rm -fv tsconfig*.tsbuildinfo",
    "build:dev": "tsc && vite build --mode development && rm -fv tsconfig*.tsbuildinfo",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "ahooks": "^3.8.5",
    "antd": "^5.25.4",
    "axios": "1.8.2",
    "date-fns": "^4.1.0",
    "echarts": "^5.6.0",
    "less": "^4.3.0",
    "less-loader": "^12.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-draggable": "^4.4.6",
    "react-router-dom": "^7.6.2",
    "resso": "^0.22.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@types/lodash": "^4.17.17",
    "@types/node": "22.9.0",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^4.5.1",
    "eslint": "9.14.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.33.1",
    "vite": "6.2.6"
  }
}
